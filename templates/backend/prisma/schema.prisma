// schema.prisma

// 定义数据库连接和 Prisma client 配置
datasource db {
  provider = "postgresql" // 根据实际使用的数据库修改
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

// 定义角色枚举
enum RoleLevel {
    ADMIN
    USER
}

model User {
  id        String    @id @default(cuid())
  username String     @unique
  password String
  role     RoleLevel? @default(USER) // 默认角色设置为 GUEST
}
